<script>
  (function () {
    function initContactForm() {
      var form = document.getElementById('contactFormPortfolio');
      var sendFormStatus = document.getElementById('send_contact_form_status');
      if (!form || !sendFormStatus || form.dataset.bound === 'true') return;
      form.dataset.bound = 'true';

      form.addEventListener('submit', function (e) {
        e.preventDefault();
        sendFormStatus.style.display = 'block';
        sendFormStatus.innerHTML = 'Wyslano formularz kontaktowy';
        var formData = new FormData(form);
        var xhr = new XMLHttpRequest();
        var url = form.getAttribute('action') || 'https://www.futurewebstudio.pl/contactFormPortfolio.php';
        xhr.open('POST', url, true);
        xhr.onreadystatechange = function () {
          if (xhr.readyState === XMLHttpRequest.DONE) {
            if (xhr.status === 200) {
              try {
                var res = JSON.parse(xhr.responseText);
                var msg = res.msg || res.message;
                if (msg) sendFormStatus.innerHTML = msg;
                if (res.status === 1) form.reset();
              } catch (err) {
                sendFormStatus.innerHTML = 'Blad podczas przetwarzania odpowiedzi.';
              }
            } else {
              sendFormStatus.innerHTML = 'Wystapil blad podczas wysylania formularza.';
            }
          }
        };
        xhr.send(formData);
      });
    }

    document.addEventListener('DOMContentLoaded', initContactForm);
    if (window.barba && window.barba.hooks && window.barba.hooks.after) {
      window.barba.hooks.after(initContactForm);
    }
  })();
</script>

<script>
  (function () {
    function updateLocalTime() {
      var el = document.getElementById('timeSpan');
      if (!el) return;
      el.textContent = new Intl.DateTimeFormat('pl-PL', {
        hour: '2-digit',
        minute: '2-digit',
      }).format(new Date());
    }
    document.addEventListener('DOMContentLoaded', function () {
      updateLocalTime();
      setInterval(updateLocalTime, 60000);
    });
  })();
</script>



