--- 
title: "Realizacje" 
loadingLabel: "Realizacje"
permalink: "/work.html" 
--- 

{% include "partials/header.njk" %} 

<div class="main-wrap" data-scroll-container>
  <header class="section default-header work-header" data-scroll-section>
    <div class="nav-bar">
      <div class="credits-top">
        <div class="btn btn-link btn-left-top">
          <a href="index.html" class="btn-click magnetic" data-strength="20" data-strength-text="10">
            <span class="btn-text">
              <div class="credit">
                <span>Â©</span>
              </div>
              <div class="cbd">
                <span class="code-by">Code by </span>
                <span class="dennis">
                  <span class="dennis-span">Przemek</span>
                  <span class="snellenberg">Miros</span>
                </span>
            </span>
        </div>
        </span>
        </a>
      </div>
    </div>
    <ul class="links-wrap">
      <li class="btn btn-link active">
        <a href="work.html" class="btn-click magnetic" data-strength="20" data-strength-text="10">
          <span class="btn-text">
            <span class="btn-text-inner">Work</span>
          </span>
        </a>
      </li>
      <li class="btn btn-link">
        <a href="about.html" class="btn-click magnetic" data-strength="20" data-strength-text="10">
          <span class="btn-text">
            <span class="btn-text-inner">About</span>
          </span>
        </a>
      </li>
      <li class="btn btn-link">
        <a href="contact.html" class="btn-click magnetic" data-strength="20" data-strength-text="10">
          <span class="btn-text">
            <span class="btn-text-inner">Contact</span>
          </span>
        </a>
      </li>
      <li class="btn btn-link btn-menu">
        <div class="btn-click magnetic" data-strength="20" data-strength-text="10">
          <div class="btn-text">
            <span class="btn-text-inner">Menu</span>
          </div>
        </div>
      </li>
    </ul>
</div>
<div class="container medium">
  <div class="row">
    <div class="flex-col once-in">
      <h1>
        <span>Zobacz moje </span>
        <span>wybrane realizacje</span>
      </h1>
    </div>
  </div>
</div>
</header>
<section class="section work-filters" data-scroll-section>
  <div class="container once-in">
    <div class="filter-row">
      <div class="toggle-row">
        <div class="btn btn-normal all-btn active">
          <div class="btn-click magnetic" data-strength="25" data-strength-text="15">
            <div class="btn-fill"></div>
            <span class="btn-text">
              <span class="btn-text-inner change">Wszystkie</span>
            </span>
          </div>
        </div>
        <div class="btn btn-normal design-btn">
          <div class="btn-click magnetic" data-strength="25" data-strength-text="15">
            <div class="btn-fill"></div>
            <span class="btn-text">
              <span class="btn-text-inner change">Strony www <div class="count-nr">7</div>
              </span>
            </span>
          </div>
        </div>
        <div class="btn btn-normal development-btn">
          <div class="btn-click magnetic" data-strength="25" data-strength-text="15">
            <div class="btn-fill"></div>
            <span class="btn-text">
              <span class="btn-text-inner change">Ecommerce <div class="count-nr">11</div>
              </span>
            </span>
          </div>
        </div>
      </div>
      <div class="grid-row">
        <div class="btn btn-normal btn-icon rows-btn active">
          <div class="btn-click magnetic" data-strength="25" data-strength-text="15">
            <div class="btn-fill"></div>
            <span class="btn-text">
              <span class="btn-text-inner change">
                <svg style="width: 20px;" width="20" height="19" viewBox="0 0 20 19">
                  <g fill="currentColor" fill-rule="evenodd">
                    <path d="M0 6h20v1H0zM0 0h20v1H0zM0 12h20v1H0zM0 18h20v1H0z" />
                  </g>
                </svg>
              </span>
            </span>
          </div>
        </div>
        <div class="btn btn-normal btn-icon columns-btn">
          <div class="btn-click magnetic" data-strength="25" data-strength-text="15">
            <div class="btn-fill"></div>
            <span class="btn-text">
              <span class="btn-text-inner change">
                <svg style="width: 20px;" width="20" height="20" viewBox="0 0 20 20">
                  <g fill="currentColor" fill-rule="nonzero">
                    <path d="M8 0H0v8h8V0zM7 1v6H1V1h6zM8 12H0v8h8v-8zm-1 1v6H1v-6h6zM20 0h-8v8h8V0zm-1 1v6h-6V1h6zM20 12h-8v8h8v-8zm-1 1v6h-6v-6h6z" />
                  </g>
                </svg>
              </span>
            </span>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<section class="section-wrap section-wrap-work once-in" data-scroll-section>
  <section class="section work-grid small-work-grid grid-fade grid-rows-part visible">
    <div class="container">
      <div class="grid-sub-title">
        <div class="flex-col">
          <h5>Client</h5>
        </div>
        <div class="flex-col">
          <h5>Location</h5>
        </div>
        <div class="flex-col">
          <h5>Services</h5>
        </div>
        <div class="flex-col">
          <h5>Year</h5>
        </div>
      </div>
      {% set worksList = collections.works or [] %}
      <ul class="work-items mouse-pos-list-image-wrap all-active">
        {% for work in worksList %}
        <li class="{{ (work.data.classes or []) | join(' ') }}">
          <div class="stripe animate"></div>
          <a href="{{ work.data.href }}" class="row">
            <div class="flex-col">
              <h4>
                <span>{{ work.data.tileTitle or work.data.title }}</span>
              </h4>
            </div>
            <div class="flex-col animate">
              <p>{{ work.data.location }}</p>
            </div>
            <div class="flex-col animate">
              <p>{{ work.data.role }}</p>
            </div>
            <div class="flex-col animate">
              <p>{{ work.data.year }}</p>
            </div>
          </a>
        </li>
        {% endfor %}
        <div class="stripe last animate"></div>
      </ul>
    </div>
  </section>
  <section class="section work-tiles grid-fade grid-columns-part">
    <div class="container">
      {% set worksList = worksList or collections.works or [] %}
      <ul>
        {% for work in worksList %}
        <li class="{{ (work.data.classes or []) | join(' ') }}">
          <div class="single-tile-wrap">
            <a href="{{ work.data.href }}" class="row">
              <div class="flex-col">
                <div class="tile-image">
                  <div class="overlay overlay-color" style="background-color: {{ work.data.tileBg or '#F1F1F1' }};"></div>
                  <div class="overlay overlay-image lazy" style="background-position: center center; background-repeat: no-repeat; background-size: cover;" data-bg="{{ work.data.tileImage }}"></div>
                </div>
              </div>
              <div class="flex-col">
                <h4>
                  <span>{{ work.data.tileTitle or work.data.title }}</span>
                </h4>
                <div class="stripe"></div>
              </div>
              <div class="flex-col">
                <p>{{ work.data.role }}</p>
              </div>
              <div class="flex-col">
                <p>{{ work.data.year }}</p>
              </div>
            </a>
          </div>
        </li>
        {% endfor %}
      </ul>
    </div>
  </section>
{% set archiveCount = (collections.works or []) | length %}
</section>
<section class="section center-grid-btn center-grid-btn-archive" data-scroll-section>
  <div class="container">
    <div class="grid-after-btn">
      <div class="btn btn-normal btn-dark">
        <a href="archive.html" class="btn-click magnetic" data-strength="25" data-strength-text="15">
          <div class="btn-fill"></div>
          <span class="btn-text">
            <span class="btn-text-inner change">Archive <div class="count-nr">{{ archiveCount }}</div>
            </span>
          </span>
        </a>
      </div>
    </div>
  </div>
</section>

{% include "partials/footer.njk" %} 
